# API Keys # 此处填写您的API密钥
DASHSCOPE_API_KEY=your_dashscope_api_key 
DEEPSEEK_API_KEY=your_deepseek_api_key

# API URLs and Models # 此处填写您的API基础URL和模型。我们在这里使用了阿里的Qwen VL-OCR 作为OCR模型，Deepseek Chat 作为文本处理模型
DASHSCOPE_API_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_MODEL=qwen-vl-ocr
DEEPSEEK_API_BASE_URL=https://api.lapis.cafe/v1
DEEPSEEK_MODEL=deepseek-chat

# API Parameters # 此处填写您的API参数
DEEPSEEK_TEMPERATURE=0.3
DEEPSEEK_MAX_TOKENS=2000

# Directory Settings # 此处填写您的目录设置
INPUT_DIR=input
OUTPUT_DIR=output
PDF_FILENAME=notes.pdf
TXT_FILENAME=notes.txt

# Image Processing Settings # 此处填写您的图像处理设置
MIN_PAGE_AREA_RATIO=0.15
PAGE_MARGIN=10
MIN_ASPECT_RATIO=0.5
MAX_ASPECT_RATIO=1.5
MIN_IMAGE_SIZE=1000

# OCR Settings # 此处填写您的OCR设置
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=2

# Supported Image Formats (comma-separated) # 此处填写您支持的图像格式（逗号分隔）
SUPPORTED_FORMATS=.jpg,.jpeg,.png
